{
  "name": "Open Tabs",
  "description": "Manage your open tabs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "bookmarks",
    "history"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}